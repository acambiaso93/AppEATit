<div class="container-landing-page">
  <div class="caption" style="display: inline-block;"><span>Discover the best recipes<br> that your fridge has to offer</span>
<div id="login" style="display: inline-block;">
      <% if user_signed_in? %>
        <%= link_to "Dashboard", kitchen_path, class: "button-main" %>
        <%= link_to "Logout", destroy_user_session_path, data: {turbo_method: :delete}, class: "button-main" %>
      <% else %>
        <%= link_to "Login", new_user_session_path, class: "button-main" %>
        <%= link_to "Register", new_user_registration_path, class: "button-main" %>
      <% end  %>
    </div>
  </div>
  <div class="fridge">
  <span data-controller="typed"></span>
  </div>
  <div class="refrigerator">
  </div>
  <div class="card card-one"></div>
  <div class="card card-two"></div>
  <div class="card card-three"></div>
  <div class="card card-four"></div>
  <div class="footer"></div>
</div>
