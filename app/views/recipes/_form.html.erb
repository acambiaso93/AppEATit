<div>
  <%= simple_form_for[recipe], url: dashboard_path, html: { method: :post } do |f| %>
    <%= f.input :name %>
    <%= f.input :instructions %>
    <%= f.input :prep_time %>
    <%= f.input :cook_time %>
    <%= f.input :total_time %>
    <%= f.input :difficulty %>
    <%= f.input :servings %>
    <%= f.input :image %>
    <%= f.input :video %>
    <%= f.input :created_at %>
    <%= f.hidden_field :user_id, value: current_user.id %>
    <%= f.button :submit, class: "button-main" %>
  <% end %>
</div>
